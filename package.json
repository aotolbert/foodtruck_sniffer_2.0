    "firebase": "^5.6.0",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.17",
    "mysql2": "^1.6.4",
    "react": "^16.6.3",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "^4.3.1",
